#!/Users/TOMEK/Documents/PythonProjects/virtualenvs/djanngo_python3/bin/python3.6
# EASY-INSTALL-ENTRY-SCRIPT: 'email-validator==1.0.2','console_scripts','email_validator'
__requires__ = 'email-validator==1.0.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('email-validator==1.0.2', 'console_scripts', 'email_validator')()
    )
